Algoritmo "maior numero"

Var cont,n ,s,maior :inteiro



Inicio
cont <-1
s <- 0
Enquanto (cont <= 10 ) faca
escreva("digite o ",cont,"o.valor : ")
leia(n)
 se (n > maior)entao
 maior <- n
 fimse
 s <- s + n
cont <- cont +1
fimenquanto
 escreval("a soma de todos os valores foi ",s)
 escreval("o maior valor digitado foi ", maior)
Fimalgoritmo